{
	"name": "sparse-octree",
	"version": "7.0.1",
	"description": "A sparse octree data structure.",
	"homepage": "https://github.com/vanruesc/sparse-octree",
	"main": "dist/sparse-octree.js",
	"module": "dist/sparse-octree.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": "./dist/sparse-octree.js"
	},
	"type": "module",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"partition",
		"hierarchy",
		"subdivision",
		"raycasting",
		"3d",
		"spatial",
		"voxel",
		"points",
		"frustum",
		"culling",
		"pointer"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/sparse-octree.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/sparse-octree/issues"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">= 0.14.0"
	},
	"scripts": {
		"ava": "ava",
		"build": "run-s build:*",
		"build:js": "node esbuild -m",
		"build:dts": "tsc -p tsconfig.d.json",
		"clean": "del-cli dist public",
		"copy": "cpy --cwd=demo/static --parents \"**/*\" \"../../public/demo\"",
		"deploy": "run-s copy lint build doc",
		"dev": "run-s clean copy && run-p start watch",
		"doc": "typedoc",
		"lint": "tsc --noemit && eslint --ext .ts --fix src demo/src",
		"prepublishOnly": "run-s clean test doc",
		"test": "run-s lint build ava",
		"start": "hs -s",
		"watch": "run-p watch:*",
		"watch:eslint": "esw -w --ext .ts --color src demo/src",
		"watch:js": "node esbuild -w"
	},
	"ava": {
		"failFast": true,
		"files": [
			"test/**/*.js"
		]
	},
	"eslintConfig": {
		"root": true,
		"extends": "aether/typescript",
		"parserOptions": {
			"project": [
				"tsconfig.json"
			]
		}
	},
	"peerDependencies": {
		"three": ">= 0.126.0 < 0.129.0"
	},
	"devDependencies": {
		"@types/dat.gui": "0.x.x",
		"@types/three": "0.x.x",
		"@typescript-eslint/eslint-plugin": "4.x.x",
		"@typescript-eslint/parser": "4.x.x",
		"ava": "3.x.x",
		"cpy-cli": "3.x.x",
		"dat.gui": "0.x.x",
		"del-cli": "3.x.x",
		"esbuild": "0.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"eslint-watch": "7.x.x",
		"http-server": "0.x.x",
		"npm-run-all": "4.x.x",
		"spatial-controls": "4.x.x",
		"three": "0.x.x",
		"three-demo": "5.x.x",
		"tslib": "2.x.x",
		"typedoc": "0.x.x",
		"typescript": "4.x.x"
	}
}
