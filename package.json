{
	"name": "sparse-octree",
	"version": "5.0.2",
	"description": "A sparse octree data structure.",
	"homepage": "https://github.com/vanruesc/sparse-octree",
	"main": "build/sparse-octree.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [
		"partition",
		"hierarchy",
		"subdivision",
		"raycasting",
		"3d",
		"spatial",
		"voxel",
		"points",
		"frustum",
		"culling"
	],

	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},

	"repository": {
		"type": "git",
